{
  "crons": [
    {
      "path": "/api/cron/update-dollar",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/cron/update-embi",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/cron/update-emae",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/cron/update-ipc",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/cron/update-labor-market",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/update-poverty",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/update-bcra-indices",
      "schedule": "0 */12 * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "x-vercel-cron", "value": "true" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization" }
      ]
    }
  ],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "",
    "SUPABASE_SERVICE_ROLE_KEY": "",
    "CRON_SECRET_KEY": "",
    "API_SECRET_KEY": ""
  }
}